<h1>{{profileID.name}}</h1>
<img src="{{profileID.imagePath}}" alt="{{profileID.imageName}}" style="width: 110px; height: 110px">

{{#each wod}}
<div>
  <h3><a href="/wod/{{_id}}">{{wodName}}</a></h3>
  <h4>{{wodFocus}}</h4>
  <h5>{{duration}}</h5>
  <h5>{{desc}}</h5>
  <p>{{level}}</p>
  <p>{{record}}</p>
</div>
<hr>
{{/each}}

<a href="/profile/all">Vuelve a Todos los Perfiles</a>
<h4><a href="/profile"><button>Volver a mi Perfil</button></a></h4>
